---
const title = 'Latin Polytechnic Sample Text Blog: Home';
const response = await fetch('https://jsonplaceholder.typicode.com/posts');
const data = await response.json();

//check check
console.log(data);
---

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{title}</title>
</head>
<body>
	<h1>{title}</h1>
	<ul>
		{data.map(post => (
			<li key={post.id}>
				<h1>User ID: {post.userId}</h1>
				<h2>{post.id}: {post.title}</h2>
				<p>{post.body}</p>
			</li>
		))}
	</ul>
</body>
</html>